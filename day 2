using namespace std;
int main(){
    int row=3;
    int col=4;
    for(int i=1;i<=row;i++){
        for(int j=1;j<=i;j++){
            cout<<"*"<<" ";
        }
        cout<<"\n";
    }
    
}


mirror of right angled triangle && triangle
#include<iostream>
using namespace std;
int main(){
    int row=4;
    
    for(int i=1;i<=row;i++){
        for(int k=1;k<=row-i;k++){
            cout<<" ";
        }
      for(int j=1;j<=i;j++){
        cout<<"*";
    } 
    cout<<"\n";
    }
    
    
}
butterfly star
#include<iostream>
using namespace std;
int main(){
    int row=4;
    for(int i=1;i<=row;i++){
        for(int j=1;j<=i;j++){
            cout<<"*";
        }
        for(int k=1;k<=2*(row-i);k++){
            cout<<" ";
        }
        for(int j=1;j<=i;j++){
            cout<<"*";
        }
        cout<<"\n";
    }
     for(int i=row-1;i>=1;i--){
        for(int j=1;j<=i;j++){
            cout<<"*";
        }
        for(int k=1;k<=2*(row-i);k++){
            cout<<" ";
        }
        for(int j=1;j<=i;j++){
            cout<<"*";
        }
        cout<<"\n";
    }
}

rhombus
#include<iostream>
using namespace std;
int main(){
    int row=4;
    for(int i=1;i<=row;i++){
        for(int j=1;j<=row-i;j++){
            cout<<" ";
        }
        for(int k=1;k<=row;k++){
            cout<<"*";
        }
        cout<<"\n";
    }
}

diamond the triangle
#include<iostream>
using namespace std;
int main(){
    int r=5;
    for(int i=1;i<=r;i++){
        for(int j=1;j<=r-i;j++){
            cout<<" ";
        }
        for(int k=1;k<=i;k++){
            cout<<"* ";
        }
        cout<<"\n";
    }
    for(int i=r-1;i>=1;i--){
        for(int j=1;j<=r-i;j++){
            cout<<" ";
        }
        for(int k=1;k<=i;k++){
            cout<<"* ";
        }
        cout<<"\n";
    }
}

pyramid
#include<iostream>
using namespace std;
int main(){
    int row=5;
    for(int i=1;i<=row;i++){
        for(int k=1;k<=row-i;k++){
            cout<<" ";
        }
        for(int j=1;j<=2*i-1;j++){
            if(j==1||j==2*i-1||i==row){
            cout<<"*";
            }
            else
            {
                cout<<" ";
            }
        }
        cout<<"\n";
    }
    
}

hollow pyramid
#include<iostream>
using namespace std;
int main(){
    int row=5;
    for(int i=1;i<=row;i++){
        for(int k=1;k<=row-i;k++){
            cout<<" ";
        }
        for(int j=1;j<=2*i-1;j++){
            if(j==1||j==2*i-1){
            cout<<"*";
            }
            else
            {
                cout<<" ";
            }
        }
        cout<<"\n";
    }
    for(int i=row-1;i>=0;i--){
        for(int k=1;k<=row-i;k++){
            cout<<" ";
        }
        for(int j=1;j<=2*i-1;j++){
            if(j==1||j==2*i-1){
            cout<<"*";
            }
            else
            {
                cout<<" ";
            }
        }
        cout<<"\n";
    }
    
}



    







